<com.boundaryproj.bottletaste.UtilFolder.AlertFolder.BoundaryLoginDialog xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:layout_gravity="bottom"
    android:background="#00000000"
    android:orientation="vertical">

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="210dp"
            android:layout_centerHorizontal="true"
            android:layout_centerVertical="true"
            android:layout_marginLeft="10dp"
            android:layout_marginRight="10dp"
            android:background="@drawable/login_alert_layer"
            android:gravity="bottom">

            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_above="@+id/relativeLayout29">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:gravity="center"
                    android:orientation="vertical">

                    <TextView
                        android:id="@+id/title"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="20dp"
                        android:layout_marginRight="20dp"
                        android:gravity="center"
                        android:lineSpacingExtra="3sp"
                        android:text="‘Boundary’이(가)
‘facebook.com’을(를) 사용하여
로그인하려고 합니다."
                        android:textColor="@android:color/black"
                        android:textSize="15sp" />

                    <TextView
                        android:id="@+id/subtitle"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="20dp"
                        android:layout_marginRight="20dp"
                        android:layout_marginTop="8dp"
                        android:gravity="center"
                        android:lineSpacingExtra="3sp"
                        android:text="사용자에 관한 정보를 앱 및 웹 사이트가
공유하게 됩니다."
                        android:textColor="@android:color/black"
                        android:textSize="13sp" />
                </LinearLayout>
            </RelativeLayout>

            <RelativeLayout
                android:id="@+id/relativeLayout29"
                android:layout_width="match_parent"
                android:layout_height="50dp"
                android:layout_alignParentBottom="true">

                <TextView
                    android:id="@+id/positive"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentBottom="true"
                    android:layout_alignParentEnd="true"
                    android:layout_alignParentRight="true"
                    android:layout_below="@+id/relativeLayout30"
                    android:layout_toRightOf="@+id/negative"
                    android:gravity="center"
                    android:text="계속"
                    android:textColor="@color/boundary_blue"
                    android:textSize="15sp" />

                <RelativeLayout
                    android:id="@+id/relativeLayout31"
                    android:layout_width="0.5dp"
                    android:layout_height="match_parent"
                    android:layout_below="@+id/relativeLayout30"
                    android:layout_centerHorizontal="true"
                    android:background="#95989a">

                </RelativeLayout>

                <RelativeLayout
                    android:id="@+id/relativeLayout30"
                    android:layout_width="match_parent"
                    android:layout_height="0.5dp"
                    android:background="#95989a">

                </RelativeLayout>

                <TextView
                    android:id="@+id/negative"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentBottom="true"
                    android:layout_alignParentLeft="true"
                    android:layout_alignParentStart="true"
                    android:layout_alignParentTop="true"
                    android:layout_toLeftOf="@+id/relativeLayout31"
                    android:gravity="center"
                    android:text="취소"
                    android:textColor="@color/boundary_blue"
                    android:textSize="15sp" />
            </RelativeLayout>
        </RelativeLayout>
    </RelativeLayout>


</com.boundaryproj.bottletaste.UtilFolder.AlertFolder.BoundaryLoginDialog>